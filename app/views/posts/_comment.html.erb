<li>
  <%= comment.body %>
  <h6>written by <%=comment.user.username%> <%= time_ago_in_words(comment.created_at) %></h6>
  <% if current_user == comment.user %>
    <%= link_to "Edit", edit_comment_path(comment.id) %>
    <%= link_to "Delete", comment_path(comment.id), method: :delete %>
  <% end %>
</li>
